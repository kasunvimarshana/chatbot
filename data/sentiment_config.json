{
    "sentiment_map": {
        "positive": ["happy", "great", "awesome", "wonderful", "love"],
        "negative": ["sad", "angry", "hate", "awful", "frustrated"],
        "neutral": ["ok", "fine", "alright", "maybe", "whatever"]
    },
    "model_thresholds": {
        "positive": 0.75,
        "negative": 0.75
    },
    "response_templates": {
        "positive": ["Great to hear!", "That's wonderful!"],
        "negative": ["I'm sorry", "That sounds difficult"],
        "neutral": ["I see", "Tell me more"],
        "error": ["Could you rephrase that?"]
    },
    "analysis_methods": {
        "keyword_first": true,
        "fallback_to_model": true
    },
    "custom_rules": [
        {
            "pattern": "not happy",
            "sentiment": "negative",
            "override": true
        }
    ],
    "logging_level": "INFO"
}
